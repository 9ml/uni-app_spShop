.shops-main {
  width: 94%;
  height: 200rpx;
  margin: 0 auto 18rpx;
  background-color: #FFFFFF;
  border-radius: 16rpx;
}
.shops-main .sm-body {
    width: 96%;
    margin: 0 auto;
    position: relative;
}
.shops-main .sm-body .sb-title {
      height: 44rpx;
      line-height: 44rpx;
      font-size: 22rpx;
}
.shops-main .sm-body .sb-goods {
      height: 140rpx;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
}
.shops-main .sm-body .sb-goods .sg-icon {
        width: 8%;
}
.shops-main .sm-body .sb-goods .sg-icon .noCheckIcon {
          width: 30rpx;
          height: 30rpx;
          margin: 0 auto;
          background-image: url(data:image/png;base64,);
          background-repeat: no-repeat;
          background-size: 100%;
}
.shops-main .sm-body .sb-goods .sg-icon .isCheckIcon {
          width: 30rpx;
          height: 30rpx;
          margin: 0 auto;
          background-image: url(data:image/png;base64,);
          background-repeat: no-repeat;
          background-size: 100%;
}
