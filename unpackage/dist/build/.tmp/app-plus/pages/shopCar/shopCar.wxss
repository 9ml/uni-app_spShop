        text-align: center;
}
.shops-main .sm-body .sb-goods .sg-img image {
          width: 146rpx;
          height: 130rpx;
}
.shops-main .sm-body .sb-goods .sg-info {
        width: 56%;
        height: 130rpx;
}
.shops-main .sm-body .sb-goods .sg-info .si-name {
          height: 60rpx;
          font-size: 22rpx;
          overflow: hidden;
}
.shops-main .sm-body .sb-goods .sg-info .si-type {
          height: 40rpx;
          line-height: 40rpx;
}
.shops-main .sm-body .sb-goods .sg-info .si-type text {
            font-size: 20rpx;
            padding: 4rpx;
            background-color: #F1F1F1;
            border-radius: 6rpx;
}
.shops-main .sm-body .sb-goods .sg-info .si-price {
          height: 40rpx;
          line-height: 40rpx;
          font-size: 20rpx;
