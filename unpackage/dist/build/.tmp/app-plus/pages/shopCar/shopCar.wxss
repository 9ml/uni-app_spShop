      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
      right: 20rpx;
      bottom: 20rpx;
}
.shops-main .sm-body .sb-nums .sn-item {
        width: 36rpx;
        height: 36rpx;
        background-color: #F1F1F1;
