.inp-main {
  width: 94%;
  height: 82rpx;
  margin: 16rpx auto 0;
  border-radius: 16rpx;
  position: relative;
  background-color: #FFFFFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.inp-main .im-left {
    width: 16%;
    height: 60rpx;
    font-size: 28rpx;
    text-align: center;
    line-height: 60rpx;
}
.inp-main .im-input {
    width: 80%;
    height: 60rpx;
}
.inp-main .im-input input {
      width: 100%;
      height: 100%;
      font-size: 28rpx;
}
.inp-main .im-right {
    width: 20%;
    height: 60rpx;
    position: absolute;
    top: 50%;
    margin-top: -30rpx;
    right: 20rpx;
    font-size: 24rpx;
    text-align: center;
    line-height: 60rpx;
    color: #FF584C;
    z-index: 9999;
}
.inp-main .isget {
    color: #666666;
}
.tips {
  width: 90%;
  height: 60rpx;
  line-height: 60rpx;
  margin: 0 auto;
  font-size: 26rpx;
}
.tips text {
    color: #FF584C;
    margin-left: 20rpx;
}
.registerBtn {
  width: 260rpx;
  height: 72rpx;
  margin: 50rpx auto;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  font-size: 30rpx;
  color: #FFFFFF;
  text-align: center;
  line-height: 72rpx;
}

