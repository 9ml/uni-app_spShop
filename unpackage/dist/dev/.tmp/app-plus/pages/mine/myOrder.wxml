<view><view class="topNav"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="t-left" bindtap="__e"><image src="../../static/btn_back_zhuce.png" mode></image></view><view class="t-main">我的订单</view><view data-event-opts="{{[['tap',[['goSearch']]]]}}" class="t-right" bindtap="__e"><image src="/static/icon_ss.png" mode></image></view></view><view class="space_nav"></view><view class="mo-chose"><block wx:for="{{choseNavList}}" wx:for-item="mc_item" wx:for-index="mc_idx" wx:key="mc_idx"><view data-event-opts="{{[['tap',[['choseEach',[mc_idx,'$0'],[[['choseNavList','',mc_idx,'cnl_name']]]]]]]}}" class="{{['mh-item',current===mc_idx?'ac':'']}}" bindtap="__e">{{''+mc_item.cnl_name+''}}</view></block></view><view class="qb-main"><block wx:for="{{myOrderInfoList}}" wx:for-item="i" wx:for-index="i_idx" wx:key="i_idx"><view class="qm-item"><view class="qi-title"><view class="qt-gType">{{''+i.orderStatus+''}}</view></view><view class="qi-goods"><view data-event-opts="{{[['tap',[['goGoodsInfo',['$0'],[[['myOrderInfoList','',i_idx,'goodsId']]]]]]]}}" class="qg-img" bindtap="__e"><image src="{{i.goodsMainImg}}" mode></image></view><view data-event-opts="{{[['tap',[['goGoodsInfo',['$0'],[[['myOrderInfoList','',i_idx,'goodsId']]]]]]]}}" class="qg-name" bindtap="__e"><view class="qn-name">{{''+i.goodsName+''}}</view><block wx:if="{{i.isTKingFonts}}"><view class="qn-sh">正在退款</view></block><block wx:if="{{i.isTKSecFonts}}"><view class="qn-sh">退款成功</view></block><block wx:if="{{i.isTKFailFonts}}"><view class="qn-sh">退款失败</view></block></view><view class="qg-price"><view class="qp-price">{{'￥'+i.lastPrice+''}}</view><view class="qp-nums">{{'X'+i.goodsAmount+''}}</view></view><view class="qg-btn"><block wx:if="{{i.isWL}}"><view data-event-opts="{{[['tap',[['goWL',['$0'],[[['myOrderInfoList','',i_idx,'logisticsCode']]]]]]]}}" class="qb-ck" bindtap="__e">查看物流</view></block><block wx:if="{{i.isZF}}"><view data-event-opts="{{[['tap',[['goPay',['$0'],[[['myOrderInfoList','',i_idx,