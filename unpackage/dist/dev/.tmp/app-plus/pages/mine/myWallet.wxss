.topNav .t-left {
  width: 20%;
  text-align: left;
  padding-left: 20rpx;
}
.topNav .t-right {
  width: 20%;
  font-size: 24rpx;
  margin-top: 40rpx;
}
.mw-all .ma-icon {
  width: 60px;
  height: 60px;
  margin: 50rpx auto 10rpx;
}
.mw-all .ma-icon image {
    width: 100%;
    height: 100%;
}
.mw-all .ma-fonts {
  font-size: 26rpx;
  text-align: center;
}
.mw-all .ma-nums {
  height: 100rpx;
  line-height: 100rpx;
  font-weight: bold;
  font-size: 50rpx;
  text-align: center;
}
.btn-all {
  margin-top: 150rpx;
}
.btn-all view {
    width: 250rpx;
    height: 60rpx;
    text-align: center;
    margin: 0 auto;
    line-height: 60rpx;
    font-size: 24rpx;
    background-repeat: no-repeat;
    background-size: 100% 100%;
    border-radius: 10rpx;
    overflow: hidden;
}
.btn-all .cz-btn {
    background-image: url(data:image/png;base64,);
    color: #FFFFFF;
}
.btn-all .tx-btn {
    margin-top: 30rpx;
    background-image: url(data:image/png;base64,);
    color: #00CB00;
}
.zd {
  font-size: 26rpx;
  text-align: center;
  margin-top: 80rpx;
