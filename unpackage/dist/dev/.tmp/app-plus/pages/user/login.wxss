.logins {
  width: 670rpx;
  height: 460rpx;
  background-color: #FFFFFF;
  margin: 30rpx auto;
  border-radius: 30rpx;
  -webkit-box-shadow: 5rpx 5rpx 10rpx #F8F8F8;
          box-shadow: 5rpx 5rpx 10rpx #F8F8F8;
}
.logins .space_inp {
    height: 10rpx;
}
.logins .inp-item {
    width: 600rpx;
    height: 60rpx;
    margin: 30rpx auto 0;
    position: relative;
    border-bottom: 2rpx solid #E5E5E5;
}
.logins .inp-item input {
      width: 100%;
      height: 100%;
      font-size: 26rpx;
      padding-left: 6rpx;
}
.logins .inp-item .getCode {
      width: 30%;
      height: 60rpx;
      font-size: 24rpx;
      color: #FF584C;
      position: absolute;
      text-align: center;
      top: 50%;
      margin-top: -30rpx;
      right: 0;
      z-index: 9999;
}
.logins .inp-item .isget {
      color: #666666;
}
.logins .chose-item {
    width: 590rpx;
    height: 60rpx;
