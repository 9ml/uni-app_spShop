        -ms-flex-pack: justify;
            justify-content: space-between;
    font-size: 26rpx;
    color: #666666;
}
.logins .loginBtn {
    width: 300rpx;
    height: 80rpx;
    margin: 40rpx auto;
    color: #FFFFFF;
    font-size: 30rpx;
    line-height: 80rpx;
    text-align: center;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: 100% 100%;
}
